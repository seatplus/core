{
    "name": "seatplus/core",
    "type": "project",
    "description": "SeAT fork to add the very special plus, to make seat even better.",
    "keywords": [
        "eve",
        "eve online",
        "character",
        "corporation",
        "alliance",
        "esi",
        "management"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-redis": "*",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/tinker": "^2.0",
        "seatplus/eseye": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "inertiajs/inertia-laravel": "^0.2",
        "laravel/socialite": "^4.1",
        "spatie/laravel-permission": "^3.0",
        "thepinecode/i18n": "^0.7",
        "tightenco/ziggy": "^0.8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",

            "Seatplus\\Web\\": "packages/seatplus/web/src/",
            "Seatplus\\Eveapi\\": "packages/seatplus/eveapi/src/",
            "Seatplus\\Auth\\": "packages/seatplus/auth/src/"
        },
        "files": [
            "packages/seatplus/web/src/Helpers/helpers.php",
            "packages/seatplus/auth/src/Helpers/helpers.php",
            "packages/seatplus/eveapi/src/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
